{
	"name": "memphrase",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"engines": {
		"node": ">=18.0.0"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"format": "prettier --write .",
		"test:e2e": "playwright test",
		"postinstall": "npm rebuild --if-present"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@playwright/test": "^1.49.1",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/adapter-vercel": "^5.7.2",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.4",
		"@types/nodemailer": "^6.4.17",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"jsdom": "^26.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"qrcode": "^1.5.4",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.2.6",
		"vitest": "^3.0.0"
	},
	"dependencies": {
		"@inlang/paraglide-js": "^2.0.0",
		"@vercel/analytics": "^1.5.0",
		"nodemailer": "^7.0.3",
		"svelte-recaptcha-v2": "^0.0.2",
		"@rollup/rollup-linux-x64-gnu": "^4.0.0"
	},
	"optionalDependencies": {
		"@rollup/rollup-darwin-arm64": "^4.0.0",
		"@rollup/rollup-darwin-x64": "^4.0.0",
		"@rollup/rollup-linux-arm64-gnu": "^4.0.0",
		"@rollup/rollup-linux-x64-gnu": "^4.0.0",
		"@rollup/rollup-win32-ia32-msvc": "^4.0.0",
		"@rollup/rollup-win32-x64-msvc": "^4.0.0"
	},
	"overrides": {
		"vite": {
			"rollup": "npm:@rollup/wasm-node"
		}
	}
}
